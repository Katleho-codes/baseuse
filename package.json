    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "postcss": "^8.5.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "web-features": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Katleho-codes/baseuse.git"
  },
  "keywords": [
    "baseline"
  ],
